{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "prettier",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "plugin:sonarjs/recommended",
      "plugin:security/recommended",
      "plugin:react-hooks/recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "prettier/prettier": ["error"]
    }
  }